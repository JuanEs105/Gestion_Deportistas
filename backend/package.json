{
  "name": "eval-deportistas-backend",
  "version": "1.0.0",
  "description": "API para sistema de evaluaciÃ³n de deportistas",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint src/ || exit 0",
    "format": "prettier --write \"src/**/*.js\"",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "generate:admin": "node src/scripts/createAdmin.js",
    "backup:db": "node src/scripts/backupDatabase.js",
    "clean:reports": "node src/scripts/cleanupReports.js",
    "clean:uploads": "node src/scripts/cleanupUploads.js"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "deportistas",
    "evaluacion",
    "api",
    "nodejs",
    "express",
    "postgresql",
    "sequelize",
    "reportes",
    "excel",
    "pdf",
    "cloudinary",
    "jwt"
  ],
  "author": "Tu Nombre <tu.email@dominio.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/eval-deportistas-backend.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/eval-deportistas-backend/issues"
  },
  "homepage": "https://github.com/tu-usuario/eval-deportistas-backend#readme"
}